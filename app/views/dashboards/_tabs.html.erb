<ul class="tabs">
  <li class="tab <%= css_class_for_action("show", "active_tab") %>">
    <%= link_to t("active", count: @dashboard.active_defence_requests.count), dashboard_path %>
  </li>
  <li class="tab <%= css_class_for_action("closed", "active_tab") %>">
    <%= link_to t("closed", count: @dashboard.closed_defence_requests.count), dashboard_closed_path %>
  </li>
  <li class="filler">
  </li>
</ul>
